{
  "name": "working-afk-simulator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.cascade.working-afk-simulator",
    "productName": "Working AFK Simulator",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "createDesktopShortcut": true
    },
    "linux": {
      "target": "AppImage"
    },
    "asar": false
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "pixi-live2d-display": "^0.4.0",
    "pixi.js": "^8.14.3",
    "url": "^0.11.4",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^29.0.0",
    "electron-builder": "^23.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  },
  "main": "dist-electron/main.js"
}